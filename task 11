        "from tkinter import *\n",
        "from tkinter import messagebox\n",
        "\n",
        "root = Tk()\n",
        "root.title('Form')\n",
        "\n",
        "Label(root, text='Name').pack()\n",
        "n = Entry(root)\n",
        "n.pack()\n",
        "\n",
        "Label(root, text='Roll No').pack()\n",
        "r = Entry(root)\n",
        "r.pack()\n",
        "\n",
        "def show():\n",
        "    name = n.get()\n",
        "    roll = r.get()\n",
        "    messagebox.showinfo(\"Student Details\",\n",
        "                        \"Name: \" + name + \"\\nRoll No: \" + roll)\n",
        "\n",
        "Button(root, text='Submit', command=show).pack()\n",
